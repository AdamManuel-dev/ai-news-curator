{
  "name": "ai-news-curator",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon",
    "dev:debug": "nodemon --inspect=0.0.0.0:9229",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky",
    "migrate:up": "ts-node -r tsconfig-paths/register src/database/migration-cli.ts up",
    "migrate:down": "ts-node -r tsconfig-paths/register src/database/migration-cli.ts down",
    "migrate:status": "ts-node -r tsconfig-paths/register src/database/migration-cli.ts status",
    "migrate:validate": "ts-node -r tsconfig-paths/register src/database/migration-cli.ts validate",
    "migrate:create": "ts-node -r tsconfig-paths/register src/database/migration-cli.ts create",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/cli.ts all",
    "seed:run": "ts-node -r tsconfig-paths/register src/database/seeds/cli.ts run",
    "seed:clear": "ts-node -r tsconfig-paths/register src/database/seeds/cli.ts clear",
    "seed:list": "ts-node -r tsconfig-paths/register src/database/seeds/cli.ts list",
    "rotate-keys": "ts-node -r tsconfig-paths/register src/scripts/rotate-keys.ts",
    "rotate-keys:auto": "ts-node -r tsconfig-paths/register src/scripts/rotate-keys.ts --auto-rotate --no-dry-run",
    "generate-certs": "ts-node -r tsconfig-paths/register src/scripts/generate-certs.ts",
    "secrets": "ts-node -r tsconfig-paths/register src/scripts/secrets-manager.ts",
    "secrets:get": "npm run secrets get",
    "secrets:set": "npm run secrets set",
    "secrets:list": "npm run secrets list",
    "secrets:validate": "npm run secrets validate",
    "secrets:rotate-expired": "npm run secrets rotate-expired"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.848.0",
    "@pinecone-database/pinecone": "^6.1.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.15.4",
    "axios": "^1.11.0",
    "cheerio": "^1.1.2",
    "commander": "^12.1.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "robots-parser": "^3.0.1",
    "user-agents": "^1.1.611",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^4.0.10"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
